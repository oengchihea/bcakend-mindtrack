{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/run.py"
    },
    {
      "src": "/(.*)",
      "dest": "/run.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FLASK_APP": "run.py",
    "FLASK_ENV": "production"
  },
  "functions": {
    "run.py": {
      "maxDuration": 30
    }
  }
}