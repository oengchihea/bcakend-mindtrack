{
  "version": 2,
  "builds": [
    {
      "src": "app/**.py",
      "use": "@vercel/python",
      "config": {
        "wsgi": "app.__init__.application"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app/__init__.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app/__init__.py"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key"
  }
}