{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "run.py"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}